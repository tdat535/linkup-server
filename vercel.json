{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/swagger/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/docs/api-documents.json",
      "dest": "/docs/api-documents.json"
    }
  ]
}
