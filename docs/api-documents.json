{
    "openapi": "3.0.0",
    "info": {
        "title": "LinkUp API",
        "version": "1.0.0",
        "description": "API documentation for LinkUp",
        "contact": {
            "name": "LinkUp",
            "url": "https://linkup-server-rust.vercel.app"
        }
    },
    "servers": [
        {
            "url": "https://linkup-server-rust.vercel.app",
            "description": "Vercel Server"
        }
    ],
    "paths": {
        "/api/auth/register": {
            "post": {
                "summary": "Đăng ký người dùng mới",
                "tags": [
                    "Auth"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Đăng ký thành công"
                    },
                    "400": {
                        "description": "Lỗi đầu vào không hợp lệ"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "summary": "Đăng nhập người dùng",
                "tags": [
                    "Auth"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Đăng nhập thành công"
                    },
                    "400": {
                        "description": "Sai email hoặc mật khẩu"
                    }
                }
            }
        },
        "/api/auth/refresh": {
            "post": {
                "summary": "Tạo access token mới từ refresh token",
                "tags": [
                    "Auth"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "RefreshToken": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Tạo thành công access token mới"
                    },
                    "400": {
                        "description": "RefreshToken hết hạn hoặc không tồn tại"
                    }
                }
            }
        },
        "/api/auth/logout": {
            "post": {
                "summary": "Đăng xuất người dùng",
                "tags": [
                    "Auth"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "RefreshToken": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Đăng xuất thành công"
                    },
                    "400": {
                        "description": "RefreshToken hết hạn hoặc không tồn tại"
                    }
                }
            }
        },
        "/api/media/getPost": {
            "get": {
                "summary": "Lấy danh sách bài viết",
                "tags": [
                    "Media"
                ],
                "responses": {
                    "200": {
                        "description": "Lấy danh sách bài viết thành công"
                    },
                    "400": {
                        "description": "Lỗi không xác định"
                    }
                }
            }
        },
        "/api/media/upPost": {
            "post": {
                "summary": "Tạo bài viết mới",
                "tags": [
                    "Media"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "content": {
                                        "type": "string"
                                    },
                                    "image": {
                                        "type": "string"
                                    },
                                    "user_id": {
                                        "type": "int"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Tạo bài viết thành công"
                    },
                    "400": {
                        "description": "Lỗi đầu vào không hợp lệ"
                    }
                }
            }
        }
    }
}